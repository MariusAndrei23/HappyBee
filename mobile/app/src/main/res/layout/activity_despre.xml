<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Despre">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="320dp"
        android:layout_height="140dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/round_corners"
        android:justificationMode="inter_word"
        android:paddingLeft="5dp"
        android:paddingTop="10dp"
        android:paddingRight="5dp"
        android:text="Acesta aplicatie este versiunea 1.0.0 .Si ne dorim sa ajutam apicultorii pentru a avea mai multa productie si ajutor cand vine de propria afacere."
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="320dp"
        android:layout_height="140dp"
        android:layout_marginBottom="112dp"
        android:background="@drawable/round_corners"
        android:justificationMode="inter_word"
        android:paddingLeft="5dp"
        android:paddingTop="10dp"
        android:paddingRight="5dp"
        android:text="Asteptam feedback si alte sfaturi in sectiunea afisata in GooglePlay. Multumim ca ati descarcat aplicatia noastra si spor la munca va dorim!"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="@string/my_text"
        android:textColor="@color/black"
        android:textSize="30dp"

        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.093" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="320dp"
        android:layout_height="120dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/round_corners"
        android:paddingLeft="5dp"
        android:paddingTop="10dp"
        android:paddingRight="5dp"
        android:justificationMode="inter_word"
        android:text="Aceasta aplicatie a fost realizata de BeeIndustries cu ajutorul proiectantului si inginerului Andrei Marius-Vasile."
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>